{
  "items": {
    "%[sd]": "%(\\d\\$)?[sd]",
    "!": "！",
    ",": "，",
    ":": "：",
    ";": "；",
    "[^.]\\.(?!\\.|ics|%s)": "。",
    "<": "<",
    ">": ">",
    "\\((?!s).*?\\)": "（.*?）|\\(\\)",
    "\"\\.+\"": "“\\.+”",
    "'\\.+'": "‘\\.+’",
    "\\*\\*[\\S\\s]+?\\*\\*": "\\*\\*[\\S\\s]+?\\*\\*",
    "\n": "\n",
    "\\.{3}": "…",
    "\\?": "？",
    "no .+ found": "^没有\\.+找到",
    "«": "«",
    "»": "»"
  },
  "exceptions": {
    "\\((?!s).*?\\)": [
      "%s DIFF LINK(S)",
      "%s ERROR(S)"
    ],
    "[^.]\\.(?!\\.|ics|%s)": [
      "ldap.example.com",
      "ldaps://ldaps.example.com/",
      "ou=People, dc=example, dc=com",
      "Replaced by 'phid.query'.",
      "Deprecated mysql.host Format"
    ],
    ":": [
      "ldaps://ldaps.example.com/",
      "JIRA base URI should include protocol (like \"https://\")."
    ],
    ",": [
      "ou=People, dc=example, dc=com",
      "user=%s, should_verify=%s",
      "user=%s, verified=%s, primary=%s, should_verify=%s"
    ]
  }
}

